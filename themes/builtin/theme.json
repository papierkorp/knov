{
  "name": "Builtin Theme",
  "version": "1.0.0",
  "author": "System",
  "description": "Default builtin theme",
  "themeSettings": {
    "darkMode": {
      "type": "boolean",
      "default": false,
      "label": "Dark Mode"
    },
    "colorScheme": {
      "type": "select",
      "options": ["default", "green", "red", "purple"],
      "default": "default",
      "label": "Color Scheme"
    },
    "customCSS": {
      "type": "textarea",
      "default": "",
      "label": "Custom CSS"
    },
    "fileView": {
      "type": "select",
      "options": ["detailed", "compact", "reader"],
      "default": "detailed",
      "label": "File View"
    },
    "fontFamily": {
      "type": "select",
      "options": ["system", "serif", "mono"],
      "default": "system",
      "label": "Font Family"
    }
  }
}
