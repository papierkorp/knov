{
  "name": "Builtin Theme",
  "version": "1.0.0",
  "author": "System",
  "description": "Default builtin theme",
  "views": {
    "base": [""],
    "settings": [""],
    "home": [""],
    "admin": [""],
    "search": [""],
    "overview": [""],
    "playground": [""],
    "history": [""],
    "latestchanges": [""],
    "browsefiles": [""],
    "fileview": ["detailed", "compact", "reader"],
    "fileedit": [""],
    "filenew": [""],
    "dashboardview": [""],
    "dashboardedit": [""],
    "dashboardnew": [""]
  },
  "themeSettings": {
    "darkMode": {
      "type": "boolean",
      "default": false,
      "label": "Dark Mode"
    },
    "colorScheme": {
      "type": "select",
      "options": ["default", "green", "red", "purple"],
      "default": "default",
      "label": "Color Scheme"
    },
    "customCSS": {
      "type": "textarea",
      "default": "",
      "label": "Custom CSS"
    },
    "fontFamily": {
      "type": "select",
      "options": ["system", "serif", "mono"],
      "default": "system",
      "label": "Font Family"
    }
  }
