{{ define "content" }}
<div id="page-filedittable">
    <div class="container">
        <h1>{{T "table editor"}}: {{ .FilePath }}</h1>
        <div
            hx-get="/api/editor/tableeditor?filepath={{ .FilePath }}"
            hx-trigger="load"
            hx-target="this"
            hx-swap="outerHTML"
        >
            {{T "loading"}}...
        </div>
    </div>
</div>
{{ end }}
